@{
 string title = "Test View!"; // определяем переменную title
 string text = "Для вставки кода в представление HTML используется символ '@' для однострочного фрагмента кода\n и '@{...some...code}' - для многострочного";// определяем переменную text
 int hour =  DateTime.Now.Hour;
}


<!DOCTYPE html>
<html>
<head>
 <title>@title</title>
 <meta charset="utf-8" />
</head>
<body>
 <h2>Тест представления, опряделяемого аргументом метода View()</h2>
 @if (hour < 12)
 {
   <h2>Утро</h2>
 } else {
    <h2>Не утро</h2>
 }

 <p>Time: @DateTime.Now.ToShortTimeString()</p>
 <p>@text</p>

@{
 string[] people = { "Tom", "Sam", "Bob" };
}
<ul>
@for (var i = 0; i < people.Length; i++)
{
    <li>@people[i]</li>
}
</ul>

<ul>
@foreach (var person in people)
{
    <li>@person</li>
}
</ul>

@functions
{
    public int Sum(int a, int b)
    {
        return a + b;
    }

    public int Square(int n) => n * n;
}
<p>Sum of 5 and 4: <b> @Sum(5, 4)</b></p>
<p>Square of 4: <b>@Square(4)</b></p>
</body>
</html>